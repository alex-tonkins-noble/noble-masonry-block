{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "noble-performs/masonry-block",
	"version": "1.0.0",
	"title": "Masonry Block",
	"category": "custom",
	"icon": "layout",
	"description": "A block used to create top-notch masonry galleries.",
	"keywords": ["noble", "performs", "gallery", "masonry", "image"],
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true,
			"enableContrastChecker": true,
			"gradients": true,
			"link": true
		}
	},
	"attributes": {
		"previewBreakpoints": {
			"type": "object",
			"default": {
				"lg": {
					"active": false,
					"label": "Desktop",
					"order": 1
				},
				"md": {
					"active": false,
					"label": "Tablet",
					"order": 2
				},
				"sm": {
					"active": false,
					"label": "Mobile",
					"order": 3
				}
			}
		}
	},
	"providesContext": {
		"noble-performs/masonry-block/previews": "previewBreakpoints"
	},
	"textdomain": "noble-masonry-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
