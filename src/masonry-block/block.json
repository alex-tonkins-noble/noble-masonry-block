{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "noble-performs/masonry-block",
	"version": "1.0.0",
	"title": "Masonry Block",
	"category": "custom",
	"icon": "layout",
	"description": "A block used to create top-notch masonry galleries.",
	"keywords": ["noble", "performs", "gallery", "masonry", "image"],
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": true,
			"enableContrastChecker": true,
			"gradients": true,
			"link": true
		}
	},
	"textdomain": "noble-masonry-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
